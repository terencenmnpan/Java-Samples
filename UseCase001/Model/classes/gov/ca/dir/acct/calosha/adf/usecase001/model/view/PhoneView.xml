<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="PhoneView"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  SelectList="ContactMethod.CONTACT_METHOD_ID, 
       ContactMethod.CONTACT_METHOD_TYPE_CODE, 
       ContactMethod.CONTACT_METHOD_VALUE, 
       ContactMethod.CREATED_BY, 
       ContactMethod.CREATED_DATE, 
       ContactMethod.DATA_SOURCE_CODE, 
       ContactMethod.EFFECTIVE_END_DATE, 
       ContactMethod.EFFECTIVE_START_DATE, 
       ContactMethod.MODIFIED_BY, 
       ContactMethod.MODIFIED_DATE, 
       ContactMethod.PARTY_ID"
  FromList="CONTACT_METHOD ContactMethod"
  Where="CONTACT_METHOD_TYPE_CODE = 'PHONE'"
  BindingStyle="OracleName"
  CustomQuery="false"
  PageIterMode="Full"
  UseGlueCode="false"
  RowClass="gov.ca.dir.acct.calosha.adf.usecase001.model.view.PhoneViewRowImpl">
  <DesignTime>
    <Attr Name="_isCodegen" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="gov.ca.dir.acct.calosha.adf.usecase001.model.view.PhoneView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <EntityUsage
    Name="ContactMethod"
    Entity="gov.ca.dir.acct.calosha.adf.common.model.entity.ContactMethod"/>
  <ViewAttribute
    Name="ContactMethodId"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="ContactMethodId"
    EntityUsage="ContactMethod"
    AliasName="CONTACT_METHOD_ID">
    <TransientExpression><![CDATA[adf.object.getSequenceNumber("CONTACT_METHOD_ID_SEQ")]]></TransientExpression>
  </ViewAttribute>
  <ViewAttribute
    Name="ContactMethodTypeCode"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="ContactMethodTypeCode"
    EntityUsage="ContactMethod"
    AliasName="CONTACT_METHOD_TYPE_CODE"
    DefaultValue="PHONE"/>
  <ViewAttribute
    Name="ContactMethodValue"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="ContactMethodValue"
    EntityUsage="ContactMethod"
    AliasName="CONTACT_METHOD_VALUE">
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="gov.ca.dir.acct.calosha.adf.usecase001.model.view.PhoneView.ContactMethodValue_LABEL"/>
        <DISPLAYWIDTH
          Value="15"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="CreatedBy"
    IsUpdateable="false"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="CreatedBy"
    EntityUsage="ContactMethod"
    AliasName="CREATED_BY"/>
  <ViewAttribute
    Name="CreatedDate"
    IsUpdateable="false"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="CreatedDate"
    EntityUsage="ContactMethod"
    AliasName="CREATED_DATE"/>
  <ViewAttribute
    Name="DataSourceCode"
    PrecisionRule="true"
    EntityAttrName="DataSourceCode"
    EntityUsage="ContactMethod"
    AliasName="DATA_SOURCE_CODE"/>
  <ViewAttribute
    Name="EffectiveEndDate"
    PrecisionRule="true"
    EntityAttrName="EffectiveEndDate"
    EntityUsage="ContactMethod"
    AliasName="EFFECTIVE_END_DATE"/>
  <ViewAttribute
    Name="EffectiveStartDate"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="EffectiveStartDate"
    EntityUsage="ContactMethod"
    AliasName="EFFECTIVE_START_DATE">
    <TransientExpression><![CDATA[today()]]></TransientExpression>
  </ViewAttribute>
  <ViewAttribute
    Name="ModifiedBy"
    IsUpdateable="false"
    PrecisionRule="true"
    EntityAttrName="ModifiedBy"
    EntityUsage="ContactMethod"
    AliasName="MODIFIED_BY"/>
  <ViewAttribute
    Name="ModifiedDate"
    IsUpdateable="false"
    PrecisionRule="true"
    EntityAttrName="ModifiedDate"
    EntityUsage="ContactMethod"
    AliasName="MODIFIED_DATE"/>
  <ViewAttribute
    Name="PartyId"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="PartyId"
    EntityUsage="ContactMethod"
    AliasName="PARTY_ID"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="gov.ca.dir.acct.calosha.adf.usecase001.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
