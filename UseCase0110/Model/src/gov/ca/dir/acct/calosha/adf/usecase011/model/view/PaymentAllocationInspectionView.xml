  <SQLQuery>
    <![CDATA[SELECT DISTINCT
       payment_id,
       NVL (payment_allocation.inspection_id, unbilled_inspection_id)
          inspection_id,
       inspection_number
  FROM payment_allocation
       INNER JOIN inspection
          ON payment_allocation.inspection_id = inspection.inspection_id or unbilled_inspection_id = inspection.inspection_id

WHERE 
(payment_allocation.status <> 'TRANSFERRED' or payment_allocation.status is null) 
AND ALLOCATION_TYPE_CODE <> 'CORRECT']]>
  </SQLQuery>
