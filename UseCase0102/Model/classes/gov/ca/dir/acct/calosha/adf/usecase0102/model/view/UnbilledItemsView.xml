  <SQLQuery>
    <![CDATA[/* Formatted on 6/3/2014 9:55:23 AM (QP5 v5.240.12305.39446) */
  SELECT UNBILLED_ITEM.INSPECTION_ID,
         UNBILLED_ITEM.UNBILLED_ITEM_ID,
         PARTY_AGENCY_FUND_ID,
         SETUP_DATE,
         FROM_DATE,
         THROUGH_DATE,
         UNBILLED_AMOUNT,
         UNBILLED_ITEM.STATUS,
         UNBILLED_ITEM.COMMENTS,
         BILL_TYPE_CODE,
         REVENUE_SOURCE_CODE,
         FUND_ID,
         FUND_SOURCE,
         FUND_DETAIL,
         PCA_CODE,
         FISCAL_YEAR,
         AGENCY_SOURCE_CODE,
         UNBILLED_AMOUNT - SUM (NVL(ALLOCATED_AMOUNT,0)) BALANCE
    FROM CALOSHA.PARTY_AGENCY_FUND_MAP
         INNER JOIN CALOSHA.UNBILLED_ITEM
            ON (PARTY_AGENCY_FUND_MAP_ID = PARTY_AGENCY_FUND_ID)
         LEFT OUTER JOIN
         CALOSHA.PAYMENT_ALLOCATION
            ON     (UNBILLED_ITEM.UNBILLED_ITEM_ID =
                       PAYMENT_ALLOCATION.UNBILLED_ITEM_ID)
               AND (UNBILLED_ITEM.INSPECTION_ID = UNBILLED_INSPECTION_ID)
    WHERE UNBILLED_ITEM.STATUS != 'CANCELLED' OR UNBILLED_ITEM.STATUS IS NULL
    AND ALLOCATION_TYPE_CODE = 'REGULAR'
GROUP BY UNBILLED_ITEM.INSPECTION_ID,
         UNBILLED_ITEM.UNBILLED_ITEM_ID,
         PARTY_AGENCY_FUND_ID,
         SETUP_DATE,
         FROM_DATE,
         THROUGH_DATE,
         UNBILLED_AMOUNT,
         UNBILLED_ITEM.STATUS,
         UNBILLED_ITEM.COMMENTS,
         BILL_TYPE_CODE,
         REVENUE_SOURCE_CODE,
         FUND_ID,
         FUND_SOURCE,
         FUND_DETAIL,
         PCA_CODE,
         FISCAL_YEAR,
         AGENCY_SOURCE_CODE]]>
  </SQLQuery>
